 "conda-forge.yml": {},
 "feedstock_name": "ligo-gracedb",
 "hash_type": "sha256",
 "linux_64_meta_yaml": {
  "about": {
   "description": "The Gravitational-wave Candidate Event Database (GraceDB) is a\nweb service designed for aggregating and communicating information\nabout candidate events from gravitational-wave searches and\nassociated follow-ups.  `ligo-gracedb` provides a Python-based\nclient tool for facilitating interactions with the GraceDB API.\n",
   "dev_url": "https://git.ligo.org/lscsoft/gracedb-client",
   "doc_url": "https://ligo-gracedb.readthedocs.io/",
   "home": "https://ligo-gracedb.readthedocs.io/",
   "license": "GPL-3.0-or-later",
   "license_family": "GPL",
   "license_file": "LICENSE",
   "summary": "A Python package for accessing the GraceDB API"
  },
  "build": {
   "entry_points": [
    "gracedb = ligo.gracedb.cli.client:main",
    "gracedb_legacy = ligo.gracedb.legacy_cli:main"
   ],
   "noarch": "python",
   "number": "0",
   "script": "-m pip install . -vv"
  },
  "extra": {
   "recipe-maintainers": [
    "aepace",
    "duncanmmacleod"
   ]
  },
  "package": {
   "name": "ligo-gracedb",
   "version": "2.7.0"
  },
  "requirements": {
   "host": [
    "pip",
    "python",
    "setuptools"
   ],
   "run": [
    "cryptography >=1.7.2",
    "future >=0.15.0",
    "ligo-common",
    "python",
    "requests >=2.6.0",
    "six >=1.9.0"
   ]
  },
  "source": {
   "sha256": "60a8ffa2db7d00f3eb706926e2291b03f9f51ae9e02d5b5732df2b878b4a18d7",
   "url": "https://pypi.io/packages/source/l/ligo-gracedb/ligo-gracedb-2.7.0.tar.gz"
  },
  "test": {
   "commands": [
    "gracedb --help",
    "gracedb_legacy --help",
    "gracedb search superevents \"far<1e-9\" --columns superevent_id,far,labels --force-noauth || true",
    "python -m pytest --pyargs ligo.gracedb -m \"not integration\""
   ],
   "imports": [
    "ligo.gracedb.rest"
   ],
   "requires": [
    "pytest"
   ]
  }
 },
