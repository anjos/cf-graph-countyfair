equires:\n        - pip\n\n\n  - name: dbt-redshift\n    build:\n      noarch: python\n      script: python -m pip install --no-deps -vv ./dbt-redshift\n\n    requirements:\n      host:\n        - pip\n        - python >=3.7\n      run:\n        - python >=3.7\n        # Update the version number in URL before verifying dependencies.\n        # https://github.com/dbt-labs/dbt-redshift/blob/v1.0.0/setup.py\n        - boto3 >=1.4.4,<2.0.0\n        - {{ pin_subpackage(\"dbt-core\", exact=True) }}\n        - {{ pin_subpackage(\"dbt-postgres\", min_pin='x.x', max_pin='x.x') }}\n\n    test:\n      imports:\n        - dbt\n        - dbt.adapters\n      commands:\n        - pip check\n      requires:\n        - pip\n\n  - name: dbt-bigquery\n    build:\n      noarch: python\n      script: python -m pip install --no-deps -vv ./dbt-bigquery\n\n    requirements:\n      host:\n        - pip\n        - python >=3.7\n      run:\n        - python >=3.7\n        # Update the version number in URL before verifying dependencies.\n        # https://github.com/dbt-labs/dbt-bigquery/blob/v1.0.0/setup.py\n        - {{ pin_subpackage(\"dbt-core\", exact=True) }}\n        - protobuf >=3.13.0,<4\n        - google-cloud-core >=1.3.0,<3\n        - google-cloud-bigquery >=1.25.0,<3\n        - google-api-core >=1.16.0,<3\n        - googleapis-common-protos >=1.6.0,<2\n\n    test:\n      imports:\n        - dbt\n        - dbt.adapters\n      commands:\n        - pip check\n      requires:\n        - pip\n\n  - name: dbt-snowflake\n    build:\n      noarch: python\n      script: python -m pip install --no-deps -vv ./dbt-snowflake\n\n    requirements:\n      host:\n        - pip\n        - python >=3.7\n      run:\n        - python >=3.7\n        # Update the version number in URL before verifying dependencies.\n        # https://github.com/dbt-labs/dbt-snowflake/blob/v1.0.0/setup.py\n        - {{ pin_subpackage(\"dbt-core\", exact=True) }}\n        - snowflake-connector-python>=2.4.1,<2.8.0\n    