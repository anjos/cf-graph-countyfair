  "about": {
   "description": "Skopeo can copy, inspect delete, sign and verify container images\nwithout requiring access to a docker daemon. It supports the local\nimage store, API v2 registries, image archive (as created by\ndocker save), OCI and OSTree and a local directory format.\n",
   "dev_url": "https://github.com/containers/skopeo",
   "doc_url": "https://github.com/containers/skopeo",
   "home": "https://github.com/containers/skopeo",
   "license": "Apache-2.0",
   "license_family": "Apache",
   "license_file": [
    "LICENSE",
    "thirdparty-licenses.txt"
   ],
   "summary": "Perform operations on container images without docker daemon."
  },
  "build": {
   "number": "0"
  },
  "extra": {
   "recipe-maintainers": [
    "epruesse",
    "bgruening",
    "mbargull"
   ]
  },
