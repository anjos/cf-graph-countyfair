 "linux_64_meta_yaml": {
  "about": {
   "description": "WebP is a method of lossy and lossless compression that can be used on a large variety of photographic,\ntranslucent and graphical images found on the web. The degree of lossy compression is adjustable so a\nuser can choose the trade-off between file size and image quality.\nlibwebp-base provides the headers and shared libraries. For cwebp and dwep,\nbinaries install libwebp.\n",
   "dev_url": "https://chromium.googlesource.com/webm/libwebp",
   "doc_url": "https://developers.google.com/speed/webp/docs/using",
   "home": "https://developers.google.com/speed/webp",
   "license": "BSD-3-Clause",
   "license_file": "COPYING",
   "summary": "WebP image library"
  },
  "build": {
   "number": "4",
   "run_exports": [
    "libwebp-base"
   ]
  },
  "extra": {
   "recipe-maintainers": [
    "carterbox",
    "dschreij",
    "ocefpaf"
   ]
  },
  "package": {
   "name": "libwebp",
   "version": "1.1.0"
  },
  "requirements": {
   "build": [
    "gnuconfig",
    "c_compiler_stub",
    "make"
   ],
   "host": [
    "giflib",
    "jpeg",
    "libpng",
    "libtiff",
    "libwebp-base 1.1.0"
   ],
   "run": [
    "giflib",
    "jpeg",
    "libpng",
    "libtiff",
    "libwebp-base 1.1.0"
   ]
  },
  "source": {
   "sha256": "98a052268cc4d5ece27f76572a7f50293f439c17a98e67c4ea0c7ed6f50ef043",
   "url": "http://storage.googleapis.com/downloads.webmproject.org/releases/webp/libwebp-1.1.0.tar.gz"
  },
  "test": {
   "commands": [
    "test -f $PREFIX/bin/cwebp",
    "test -f $PREFIX/bin/dwebp",
    "dwebp examples/test.webp"
   ],
   "source_files": [
    "examples/test.webp"
   ]
  }
 },
