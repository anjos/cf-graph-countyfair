 "raw_meta_yaml": "{% set name = \"lalmetaio\" %}\n{% set version = \"2.1.1\" %}\n{% set sha256 = \"9f9d29f2664add71a9ff21ac36f4f9c8a4fb2a3edb1e5b05106b9f99b4fe9e6e\" %}\n\n# dependencies\n{% set lal_version = \"7.1.0\" %}\n{% set metaio_version = \"8.4.0\" %}\n{% set swig_version = \"3.0.10\" %}\n\npackage:\n  name: {{ name }}-split\n  version: {{ version }}\n\nsource:\n  url: https://software.ligo.org/lscsoft/source/lalsuite/{{ name }}-{{ version }}.tar.xz\n  sha256: {{ sha256 }}\n\nbuild:\n  error_overlinking: true\n  error_overdepending: true\n  number: 0\n  skip: true  # [win]\n\nrequirements:\n  build:\n    - {{ compiler('c') }}\n    - help2man >=1.37\n    - make\n    - pkg-config >=0.18.0\n    - swig >={{ swig_version }}\n  host:\n    - liblal >={{ lal_version }}\n    - metaio >={{ metaio_version }}\n\noutputs:\n  - name: liblalmetaio\n    script: install-lib.sh\n    build:\n      error_overdepending: true\n      error_overlinking: true\n      run_exports:\n        - {{ pin_subpackage(\"liblalmetaio\", max_pin=\"x\") }}\n    requirements:\n      build:\n        - {{ compiler('c') }}\n        - make  # [not win]\n        - swig >={{ swig_version }}\n      host:\n        - liblal >={{ lal_version }}\n        - metaio >={{ metaio_version }}\n      run:\n        - liblal >={{ lal_version }}\n        - metaio >={{ metaio_version }}\n      run_constrained:\n        - lalmetaio >=2.0.1\n        - python-lalmetaio >=2.0.1\n    test:\n      requires:\n        - pkg-config\n      commands:\n        # check library\n        - test -f ${PREFIX}/lib/liblalmetaio${SHLIB_EXT}  # [unix]\n        # check activate variables\n        - test \"${LALMETAIO_DATADIR}\" = \"${PREFIX}/share/lalmetaio\"  # [unix]\n        # check pkg-config\n        - pkg-config --print-errors --exact-version \"${PKG_VERSION}\" lalmetaio\n    about:\n      home: https://wiki.ligo.org/Computing/LALSuite\n      doc_url: https://lscsoft.docs.ligo.org/lalsuite/lalmetaio/\n      dev_url: https://git.ligo.org/lscsoft/lalsuite.git\n      license: GPL-2.0-or-later\n      license_family: GPL\n      license_file: COPYING\n      summary: LSC Algorithm MetaIO Library shared object libraries\n      description: |\n        The LSC Algorithm MetaIO Library for gravitational wave data analysis.\n        This package contains the shared-object libraries needed to run\n        applications that use the LAL MetaIO library.\n\n  - name: python-lalmetaio\n    script: install-python.sh\n    build:\n      error_overdepending: true\n      error_overlinking: true\n      ignore_run_exports:\n        # ignore run_exports from python's recipe\n        - python\n    requirements:\n      build:\n        - {{ compiler('c') }}\n        - help2man >=1.37\n        - make\n        - pkg-config >=0.18.0\n        - swig >={{ swig_version }}\n        # extras for cross-compiling with python:\n        - python                                 # [build_platform != target_platform]\n        - cross-python_{{ target_platform }}     # [build_platform != target_platform]\n        - numpy                                  # [build_platform != target_platform]\n      host:\n        - {{ pin_subpackage('liblalmetaio', exact=True) }}\n        - numpy\n        - python\n      run:\n        - {{ pin_subpackage('liblalmetaio', exact=True) }}\n        - {{ pin_compatible('numpy') }}\n        - python\n        - python-lal >={{ lal_version }}\n    test:\n      requires:\n        - pytest >=4.0.0a0\n      source_files:\n        - test/python\n      commands:\n        - python -m pytest -ra -v test/python\n      imports:\n        - lalmetaio\n    about:\n      home: https://wiki.ligo.org/Computing/LALSuite\n      doc_url: https://lscsoft.docs.ligo.org/lalsuite/lalmetaio/\n      dev_url: https://git.ligo.org/lscsoft/lalsuite.git\n      license: GPL-2.0-or-later\n      license_family: GPL\n      license_file: COPYING\n      summary: LSC Algorithm MetaIO Library\n      description: |\n        The LSC Algorithm MetaIO Library for gravitational wave data analysis.\n        This package contains the Python bindings of the LALMetaIO C libraries\n        and additional Python modules.\n\n        Note: this package only provides the importable Python libraries.\n        All command-line interfaces, scripts, and executables are\n        part of the `lalmetaio` package.\n\n  - name: lalmetaio\n    script: install-bin.sh\n    build:\n      error_overdepending: true\n      error_overlinking: true\n      ignore_run_exports:\n        - python\n    requirements:\n      build:\n        - {{ compiler('c') }}\n        - help2man >=1.37\n        - make\n        - sed\n      host:\n        - liblal >={{ lal_version }}\n        - {{ pin_subpackage('liblalmetaio', exact=True) }}\n        - python\n      run:\n        - liblal >={{ lal_version }}\n        - {{ pin_subpackage('liblalmetaio', exact=True) }}\n        - python\n        - {{ pin_subpackage('python-lalmetaio', exact=True) }}\n    test:\n      commands:\n        - lalmetaio_version --verbose\n    about:\n      home: https://wiki.ligo.org/Computing/LALSuite\n      doc_url: https://lscsoft.docs.ligo.org/lalsuite/lalmetaio/\n      dev_url: https://git.ligo.org/lscsoft/lalsuite.git\n      license: GPL-2.0-or-later\n      license_family: GPL\n      license_file: COPYING\n      summary: LSC Algorithm MetaIO Library\n      description: |\n        The LSC Algorithm MetaIO Library for gravitational wave data analysis.\n\nabout:\n  home: https://wiki.ligo.org/Computing/LALSuite\n  doc_url: https://lscsoft.docs.ligo.org/lalsuite/lalmetaio/\n  dev_url: https://git.ligo.org/lscsoft/lalsuite.git\n  license: GPL-2.0-or-later\n  license_family: GPL\n  license_file: COPYING\n  summary: LSC Algorithm MetaIO Library\n  description: |\n    The LSC Algorithm MetaIO Library for gravitational wave data analysis.\n\nextra:\n  feedstock-name: lalmetaio\n  recipe-maintainers:\n    - duncanmmacleod\n    - skymoo\n",
