 "feedstock_name": "singularity",
 "hash_type": "sha256",
 "linux_64_meta_yaml": {
  "about": {
   "description": "Singularity is a container platform focused on supporting \"Mobility of\nCompute\"\n\nMobility of Compute encapsulates the development to compute model where\ndevelopers can work in an environment of their choosing and creation and\nwhen the developer needs additional compute resources, this environment\ncan easily be copied and executed on other platforms. Additionally as the\nprimary use case for Singularity is targeted towards computational\nportability, many of the barriers to entry of other container solutions do\nnot apply to Singularity making it an ideal solution for users (both\ncomputational and non-computational) and HPC centers.\n",
   "dev_url": "https://github.com/singularityware/singularity",
   "doc_url": "https://www.sylabs.io/docs/",
   "home": "https://www.sylabs.io/singularity/",
   "license": "BSD-3-Clause",
   "license_file": "src/github.com/sylabs/singularity/LICENSE-LBNL.md",
   "summary": "Singularity: Application containers for Linux"
  },
  "build": {
   "detect_binary_files_with_prefix": false,
   "number": "0"
  },
  "extra": {
   "recipe-maintainers": [
    "bgruening",
    "jakirkham",
    "sodre",
    "chrisburr"
   ]
  },
  "package": {
   "name": "singularity",
   "version": "3.7.1"
  },
  "requirements": {
   "build": [
    "c_compiler_stub",
    "cxx_compiler_stub",
    "cgo_compiler_stub",
    "make",
    "pkg-config",
    "go-dep",
    "sed"
   ],
   "host": [
    "libuuid",
    "libarchive",
    "libseccomp",
    "openssl"
   ],
   "run": [
    "cni-plugins >=0.7.4",
    "squashfs-tools"
   ],
   "run_constrained": null
  },
  "source": [
   {
    "folder": "src/github.com/sylabs/singularity",
    "patches": [
     "patches/0001-Make-runtime-relocatable.patch",
     "patches/0002-Fix-configuration-for-conda-forge.patch",
     "patches/0003-Use-external-CNI.patch",
     "patches/0004-Find-unsquashfs-from-PATH.diff",
     "patches/set-version-from-env.patch"
    ],
    "sha256": "82d2c65063560195ec34551931be3c325b95e8e2009e92755fd7daad346e083c",
    "url": "https://github.com/sylabs/singularity/releases/download/v3.7.1/singularity-3.7.1.tar.gz"
   }
  ],
  "test": {
   "commands": [
    "singularity --help",
    "singularity --version",
    "singularity --version | grep 3.7.1",
    "test ! -d $PREFIX/libexec/singularity/cni",
    "test -d $PREFIX/lib/cni",
    "test ! -d $PREFIX/etc/singularity/network",
    "test -f $PREFIX/etc/cni/net.d/00_bridge.conflist",
    "singularity build --sandbox lolcow docker://godlovedc/lolcow",
    "singularity --verbose --debug run -u lolcow || (singularity --verbose --debug run -u lolcow 2>&1 | grep 'Failed to create user namespace: user namespace requires to set')",
    "singularity pull library://alpine",
    "singularity --verbose --debug run -u alpine_latest.sif echo \"Hello world\""
   ]
  }
 },
