 "raw_meta_yaml": "{% set org = \"tdegeus\" %}\n{% set name = \"GMatElastoPlastic\" %}\n{% set version = \"0.2.1\" %}\n\npackage:\n  name: {{ name|lower }}-split\n  version: {{ version }}\n\nsource:\n  url: https://github.com/{{ org }}/{{ name }}/archive/v{{ version }}.tar.gz\n  sha256: c6a2d10c5ebc0a85c49487c474a1f42ccc1301edbdf85ac0cd6b878eba37a844\n\nbuild:\n  number: 1\n  script_env: SETUPTOOLS_SCM_PRETEND_VERSION={{ version }}\n  skip: True  # [python_impl == 'pypy']\n\noutputs:\n\n  - name: {{ name|lower }}\n\n    script: install.sh  # [unix]\n    script: install.bat  # [win]\n\n    requirements:\n      build:\n        - {{ compiler('cxx') }}\n        - cmake\n        - make  # [unix]\n      host:\n        - gmatelastic\n        - gmattensor\n        - xtensor\n      run:\n        - gmatelastic\n        - gmattensor\n        - xtensor\n\n    test:\n      commands:\n        - test -d ${PREFIX}/include/{{ name }}  # [unix]\n        - test -f ${PREFIX}/include/{{ name }}/Cartesian3d.h  # [unix]\n        - test -f ${PREFIX}/lib/cmake/{{ name }}/{{ name }}Config.cmake  # [unix]\n        - if exist %LIBRARY_PREFIX%\\include\\{{ name }}\\Cartesian3d.h (exit 0) else (exit 1)  # [win]\n        - if exist %LIBRARY_PREFIX%\\lib\\cmake\\{{ name }}Config.cmake (exit 0) else (exit 1)  # [win]\n\n  - name: python-{{ name|lower }}\n\n    script: install_python.sh  # [unix]\n    script: install_python.bat  # [win]\n\n    requirements:\n      build:\n        - python                              # [build_platform != target_platform]\n        - cross-python_{{ target_platform }}  # [build_platform != target_platform]\n        - numpy *                             # [build_platform != target_platform]\n        - setuptools_scm                      # [build_platform != target_platform]\n        - {{ compiler('cxx') }}\n        - cmake\n        - ninja\n      host:\n        - gmatelastic\n        - gmattensor\n        - numpy *\n        - pip\n        - pybind11\n        - python\n