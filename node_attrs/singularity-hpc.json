 "linux_64_meta_yaml": {
  "about": {
   "description": "Singularity Registry HPC (shpc) allows you to install containers as modules.\nCurrently, this includes: Lmod and Environment Modules.\n\nAnd container technologies:\n  Singularity,\n  Podman, and\n  Docker.\n\nAnd coming soon:\n  Shifter and\n  Sarus.\n\nYou can use shpc if you are:\n  a linux administrator wanting to manage containers as modules for your cluster,\n  a cluster user that wants to maintain your own folder of custom modules, or\n  a cluster user that simply wants to pull Singularity images as GitHub packages.\n\nThe library contains a collection of module recipes that will install containers,\nso you can easily use them or write your own. To see the code, head over to the repository.\nTo browse modules available as containers, see the library.\n",
   "dev_url": "https://singularity-hpc.readthedocs.io/en/latest/getting_started/developer-guide.html",
   "doc_url": "https://singularity-hpc.readthedocs.io/en/latest/index.html",
   "home": "https://singularity-hpc.readthedocs.io",
   "license": "MPL-2.0",
   "license_file": "LICENSE",
   "summary": "Local filesystem registry for containers (intended for HPC) using Lmod or Environement Modules. Works for users and admins."
  },
  "build": {
   "entry_points": [
    "shpc = shpc.client:run_shpc"
   ],
   "noarch": "python",
   "number": "0",
   "script": "/usr/share/miniconda3/envs/run_env/conda-bld/_h_env_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_pla/bin/python -m pip install . -vv"
  },
  "extra": {
   "recipe-maintainers": [
    "ngam",
    "vsoch"
   ]
  },
  "package": {
   "name": "singularity-hpc",
   "version": "0.0.57"
  },
  "requirements": {
   "host": [
    "python >=3.6",
    "pip",
    "spython >=0.2.0",
    "jinja2",
    "jsonschema",
    "ruamel.yaml",
    "requests",
    "pytest-runner",
    "pytest =>4.6.2"
   ],
   "run": [
    "python >=3.6",
    "ruamel.yaml",
    "jinja2",
    "jsonschema",
    "requests",
    "spython >=0.2.0"
   ]
  },
  "source": {
   "sha256": "4a09afd9fc5682fa101d3dc7dabd3309018b0f0b42acc8fb78e2162fcef3ad9b",
   "url": "https://github.com/singularityhub/singularity-hpc/archive/refs/tags/0.0.57.tar.gz"
  },
  "test": {
   "commands": [
    "shpc --help",
    "shpc config get module_base",
    "pip check"
   ],
   "requires": [
    "pytest",
    "pip"
   ],
   "source_files": [
    "shpc/",
    "shpc/tests"
   ]
  }
 },
