 "linux_64_meta_yaml": {
  "about": {
   "description": "Lime Suite is a collection of software supporting several hardware\nplatforms including the LimeSDR, drivers for the LMS7002M transceiver\nRFIC, and other tools for developing with LMS7-based hardware.\nInstalling the Lime Suite enables many SDR applications such as GQRX\nto work with supported hardware through the bundled SoapySDR support\nmodule.\n\nThe `liblimesuite` package contains the main library that other\npackages should develop against. The `soapysdr-module-lms7` package\ncontains the Soapy SDR module that supports Lime devices. The\n`limesuite` package provides the entire software suite, depending\non the previous packages and also providing GUI tools.\n",
   "dev_url": "https://github.com/myriadrf/LimeSuite",
   "doc_url": "https://wiki.myriadrf.org/Lime_Suite",
   "home": "http://wiki.myriadrf.org/Lime_Suite",
   "license": "Apache-2.0 AND MIT AND BSD-3-Clause",
   "license_file": [
    "COPYING",
    "external/cpp-feather-ini-parser/LICENSE",
    "external/kissFFT/COPYING"
   ],
   "summary": "Driver and GUI for LMS7002M-based (Lime) SDR platforms"
  },
  "build": {
   "number": "1"
  },
  "extra": {
   "feedstock-name": "limesuite",
   "recipe-maintainers": [
    "ryanvolz"
   ]
  },
  "outputs": [
   {
    "build": {
     "run_exports": [
      "liblimesuite"
     ]
    },
    "name": "liblimesuite",
    "requirements": {
     "build": [
      "c_compiler_stub",
      "cxx_compiler_stub",
      "cmake",
      "ninja"
     ],
     "host": [
      "libusb"
     ]
    },
    "script": "install.sh",
    "test": {
     "commands": [
      "LimeUtil --info",
      "LimeUtil --info | grep FT601",
      "LimeUtil --info | grep FX3",
      "LimeUtil --info | grep PCIEXillybus",
      "test -f $PREFIX/include/lime/LimeSuite.h",
      "test -f $PREFIX/lib/libLimeSuite${SHLIB_EXT}",
      "test -f $PREFIX/lib/udev/rules.d/64-limesuite.rules"
     ]
    }
   },
   {
    "name": "soapysdr-module-lms7",
    "requirements": {
     "build": [
      "c_compiler_stub",
      "cxx_compiler_stub",
      "cmake",
      "ninja"
     ],
     "host": [
      "liblimesuite",
      "soapysdr"
     ],
     "run": [
      "liblimesuite"
     ]
    },
    "script": "install.sh",
    "test": {
     "commands": [
      "test -f $PREFIX/lib/SoapySDR/modules0.8/libLMS7Support.so",
      "SoapySDRUtil --info"
     ]
    }
   },
   {
    "name": "limesuite",
    "requirements": {
     "build": [
      "c_compiler_stub",
      "cxx_compiler_stub",
      "cmake",
      "ninja",
      "cdt_stub"
     ],
     "host": [
      "glew",
      "liblimesuite",
      "soapysdr-module-lms7",
      "wxwidgets"
     ],
     "run": [
      "liblimesuite",
      "soapysdr-module-lms7"
     ]
    },
    "script": "install.sh",
    "test": {
     "commands": [
      "test -f $PREFIX/bin/LimeSuiteGUI",
      "LimeUtil --info",
      "SoapySDRUtil --info"
     ]
    }
   }
  ],
