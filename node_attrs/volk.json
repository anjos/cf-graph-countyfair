 "feedstock_name": "volk",
 "hash_type": "sha256",
 "linux_64_meta_yaml": {
  "about": {
   "description": "VOLK is the Vector-Optimized Library of Kernels. It is a free library,\ncurrently offered under the GPLv3, that contains kernels of hand-written\nSIMD code for different mathematical operations. Since each SIMD\narchitecture can be very different and no compiler has yet come along to\nhandle vectorization properly or highly efficiently, VOLK approaches the\nproblem differently.\n\nFor each architecture or platform that a developer wishes to vectorize for,\na new proto-kernel is added to VOLK. At runtime, VOLK will select the\ncorrect proto-kernel. In this way, the users of VOLK call a kernel for\nperforming the operation that is platform/architecture agnostic. This\nallows us to write portable SIMD code that is optimized for a variety of\nplatforms.\n",
   "dev_url": "https://github.com/gnuradio/volk",
   "doc_url": "http://libvolk.org/doxygen/index.html",
   "home": "https://github.com/gnuradio/volk",
   "license": "GPL-3.0-or-later AND Apache-2.0",
   "license_file": [
    "COPYING",
    "LICENSE_CPU_FEATURES"
   ],
   "summary": "The Vector Optimized Library of Kernels"
  },
  "build": {
   "number": "3"
  },
  "extra": {
   "feedstock-name": "volk",
   "recipe-maintainers": [
    "ryanvolz"
   ]
  },
  "outputs": [
   {
    "build": {
     "run_exports": [
      "volk"
     ]
    },
    "name": "volk",
    "requirements": {
     "build": [
      "c_compiler_stub",
      "cxx_compiler_stub",
      "cmake"
     ],
     "host": [
      "gstreamer-orc"
     ],
     "run": [
      "gstreamer-orc"
     ]
    },
    "script": "install_volk.sh",
    "test": {
     "commands": [
      "command -v volk-config-info",
      "command -v volk_profile",
      "test -f $PREFIX/lib/libvolk${SHLIB_EXT}",
      "list_cpu_features_volk",
      "volk-config-info --version --prefix --cc --cflags --all-machines --avail-machines --machine --alignment --malloc"
     ]
    }
   },
   {
    "build": {
     "entry_points": [
      "volk_modtool = volk_modtool.__main__:main"
     ],
     "noarch": "python",
     "string": "py_3"
    },
    "name": "volk-modtool",
    "requirements": {
     "build": [
      "cmake"
     ],
     "run": [
      "python"
     ]
    },
    "script": "install_volk_modtool.sh",
    "test": {
     "commands": [
      "volk_modtool"
     ],
     "imports": [
      "volk_modtool"
     ]
    }
   },
   {
    "build": {
     "run_exports": [
      "volk"
     ]
    },
    "name": "volk-dev",
    "requirements": {
     "run": [
      "volk",
      "volk-modtool"
     ]
    },
    "test": {
     "commands": [
      "volk-config-info --version --prefix --cc --cflags --all-machines --avail-machines --machine --alignment --malloc",
      "volk_modtool"
     ],
     "imports": [
      "volk_modtool"
     ]
    }
   }
  ],
  "package": {
   "name": "volk-split",
   "version": "2.4.1"
  },
  "requirements": {
   "build": [
    "c_compiler_stub",
    "cxx_compiler_stub",
    "cmake >=3.8",
    "gstreamer-orc",
    "mako >=0.4.2",
    "ninja",
    "pkg-config",
    "python >=3.6"
   ],
   "host": [
    "gstreamer-orc"
   ]
  },
  "source": {
   "patches": [
    "0001-Add-main-to-volk_modtool-script-usable-as-an-entry-p.patch",
    "0002-Fix-CMAKE_SYSTEM_PROCESSOR-arm64-should-AARCH64-not-.patch",
    "0003-Add-support-for-Apple-M1-AArch64-processor.patch"
   ],
   "sha256": "61d681f90e0f30894f806ab050e9ba28260799c01f1ed0f58623942d8c7f4d48",
   "url": "https://github.com/gnuradio/volk/releases/download/v2.4.1/volk-2.4.1.tar.gz"
  }
 },
