   "number": "0"
  },
  "extra": {
   "feedstock-name": "lalpulsar",
   "recipe-maintainers": [
    "duncanmmacleod",
    "skymoo"
   ]
  },
  "outputs": [
   {
    "about": {
     "description": "The LSC Algorithm Pulsar Library for gravitational wave data analysis.\nThis package contains the shared-object libraries needed to run\napplications that use the LAL Pulsar library.\n",
     "dev_url": "https://git.ligo.org/lscsoft/lalsuite.git",
     "doc_url": "https://lscsoft.docs.ligo.org/lalsuite/lalpulsar/",
     "home": "https://wiki.ligo.org/Computing/LALSuite",
     "license": "GPL-2.0-or-later",
     "license_family": "GPL",
     "license_file": "COPYING",
     "summary": "LSC Algorithm Pulsar Library shared object libraries"
    },
    "build": {
     "error_overdepending": true,
     "error_overlinking": true,
     "ignore_run_exports": [
      "fftw"
     ],
     "run_exports": [
      "liblalpulsar"
     ]
    },
    "name": "liblalpulsar",
    "requirements": {
     "build": [
      "c_compiler_stub",
      "libgomp",
      "make",
      "swig >=3.0.10"
     ],
     "host": [
      "cfitsio",
      "fftw * nompi*",
      "gsl",
      "liblal >=7.1.0 fftw*",
      "libgomp"
     ],
     "run": [
      "cfitsio",
      "fftw",
      "gsl",
      "liblal >=7.1.0"
     ],
     "run_constrained": [
      "lalpulsar >=3.0.0",
      "python-lalpulsar >=3.0.0"
     ]
    },
    "script": "install-lib.sh",
    "test": {
     "commands": [
      "test -f ${PREFIX}/lib/liblalpulsar${SHLIB_EXT}",
      "test \"${LALPULSAR_DATADIR}\" = \"${PREFIX}/share/lalpulsar\"",
      "pkg-config --print-errors --exact-version \"${PKG_VERSION}\" lalpulsar"
     ],
     "requires": [
      "pkg-config"
     ]
    }
   },
   {
    "about": {
     "description": "The LSC Algorithm Pulsar Library for gravitational wave data analysis.\nThis package contains the Python bindings of the LALPulsar C libraries\nand additional Python modules.\n\nNote: this package only provides the importable Python libraries.\nAll command-line interfaces, scripts, and executables are\npart of the `lalpulsar` package.\n",
     "dev_url": "https://git.ligo.org/lscsoft/lalsuite.git",
     "doc_url": "https://lscsoft.docs.ligo.org/lalsuite/lalpulsar/",
     "home": "https://wiki.ligo.org/Computing/LALSuite",
     "license": "GPL-2.0-or-later",
     "license_family": "GPL",
     "license_file": "COPYING",
     "summary": "LSC Algorithm Pulsar Library Python libraries"
    },
    "build": {
     "error_overdepending": true,
     "error_overlinking": true,
     "ignore_run_exports": [
      "python"
     ]
    },
    "name": "python-lalpulsar",
    "requirements": {
     "build": [
      "c_compiler_stub",
      "help2man >=1.37",
      "make",
      "pkg-config",
      "swig >=3.0.10"
     ],
     "host": [
      "liblalpulsar",
      "numpy",
      "python"
     ],
     "run": [
      "astropy",
      "liblalpulsar",
      "numpy",
      "python",
      "python-lal >=7.1.0",
      "six"
     ]
    },
    "script": "install-python.sh",
    "test": {
     "commands": [
      "LAL_TEST_PKGDATADIR=\"./lib\" python -m pytest -ra -v test/python"
     ],
     "imports": [
      "lalpulsar",
      "lalpulsar.NstarTools",
      "lalpulsar.PulsarParametersWrapper",
      "lalpulsar.simulateCW"
     ],
     "requires": [
      "pytest >=4.0.0a0"
     ],
     "source_files": [
      "test/python",
      "lib/*.dat.gz"
     ]
    }
   },
