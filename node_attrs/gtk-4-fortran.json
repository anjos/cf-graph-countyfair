 "linux_64_meta_yaml": {
  "about": {
   "description": "The gtk-fortran project aims to offer scientists programming in Fortran a\ncross-platform library to build Graphical User Interfaces (GUI).\nGtk-fortran is a partial GTK / Fortran binding 100% written in Fortran, thanks\nto the ISO_C_BINDING module for interoperability between C and Fortran, which\nis a part of the Fortran 2003 standard.\n",
   "doc_url": "https://github.com/vmagnin/gtk-fortran/wiki",
   "home": "https://github.com/vmagnin/gtk-fortran",
   "license": "GPL-3.0-or-later",
   "license_file": "LICENSE",
   "summary": "GTK / Fortran bindings"
  },
  "build": {
   "ignore_run_exports": [
    "plplot"
   ],
   "number": "0",
   "run_exports": [
    "gtk-4-fortran"
   ],
   "script": [
    "set -ex",
    "export FFLAGS=\"${FFLAGS} -ffree-line-length-none\"",
    "cmake ${CMAKE_ARGS} -B_build -GNinja\n",
    "cmake --build _build",
    "ctest --test-dir _build -R '^(regex|pixbuf_without_gui|tests)$'",
    "cmake --install _build"
   ]
  },
  "extra": {
   "recipe-maintainers": [
    "awvwgk",
    "vmagnin"
   ]
  },
  "package": {
   "name": "gtk-4-fortran",
   "version": "4.1.0"
  },
  "requirements": {
   "build": [
    "fortran_compiler_stub",
    "cmake",
    "ninja",
    "pkg-config"
   ],
   "host": [
    "glib",
    "gtk4",
    "plplot"
   ],
   "run": [
    "gdk-pixbuf",
    "cairo"
   ]
  },
