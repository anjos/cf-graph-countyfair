 "linux_64_meta_yaml": {
  "about": {
   "description": "GnuPG is a complete and free implementation of the OpenPGP standard as defined\nby RFC4880 (also known as PGP). GnuPG allows you to encrypt and sign your data\nand communications; it features a versatile key management system, along with\naccess modules for all kinds of public key directories. GnuPG, also known as GPG,\nis a command line tool with features for easy integration with other applications.\nA wealth of frontend applications and libraries are available. GnuPG also provides\nsupport for S/MIME and Secure Shell (ssh).\n",
   "home": "https://www.gnupg.org",
   "license": "LGPL-3",
   "license_family": "GPL",
   "license_file": "COPYING",
   "summary": "The New GNU Privacy Guard"
  },
  "build": {
   "number": "0",
   "run_exports": [
    "gnupg"
   ],
   "script": [
    "export CPPFLAGS=\"-DIN_EXCL_UNLINK=0x04000000 $CPPFLAGS\"",
    "export LDFLAGS=\"-lrt $LDFLAGS\"",
    "./configure --prefix=\"$PREFIX\"",
    "make",
    "make install"
   ]
  },
  "extra": {
   "recipe-maintainers": [
    "mariusvniekerk"
   ]
  },
  "package": {
   "name": "gnupg",
   "version": "2.3.2"
  },
