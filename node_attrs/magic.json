 "feedstock_name": "magic",
 "hash_type": "sha256",
 "linux_64_meta_yaml": {
  "about": {
   "description": "Magic is a venerable VLSI layout tool, written in the 1980's at Berkeley by John Ousterhout, now famous primarily for writing the scripting interpreter language Tcl.\nDue largely in part to its liberal Berkeley open-source license, magic has remained popular with universities and small companies.\nThe open-source license has allowed VLSI engineers with a bent toward programming to implement clever ideas and help magic stay abreast of fabrication technology.\nHowever, it is the well thought-out core algorithms which lend to magic the greatest part of its popularity.\nMagic is widely cited as being the easiest tool to use for circuit layout, even for people who ultimately rely on commercial tools for their product design flow.\n",
   "dev_url": "https://github.com/RTimothyEdwards/magic",
   "doc_url": "http://opencircuitdesign.com/magic/magic_docs.html",
   "home": "http://opencircuitdesign.com/magic/",
   "license": "MIT",
   "license_family": "MIT",
   "license_file": "LICENSE",
   "summary": "Magic VLSI Layout Tool"
  },
  "build": {
   "number": "0"
  },
  "extra": {
   "recipe-maintainers": [
    "proppy",
    "curtisma"
   ]
  },
  "package": {
   "name": "magic",
   "version": "8.3.339"
  },
  "requirements": {
   "build": [
    "autoconf",
    "automake",
    "make",
    "tcsh",
    "python",
    "c_compiler_stub"
   ],
   "host": [
    "tk",
    "xorg-libx11",
    "cairo",
    "zlib"
   ],
   "run": [
    "xorg-libx11"
   ]
  },
  "source": {
   "patches": [
    "csh.patch"
   ],
   "sha256": "2cd6e0ee88fe358beeac3a82f2cc93e2e6db08a1ee87aa22ac3e1c9a6d3c6f36",
   "url": "https://github.com/RTimothyEdwards/magic/archive/refs/tags/8.3.339.tar.gz"
  },
  "test": {
   "commands": [
    "magic -dnull -noconsole --version"
   ]
  }
 },
