 "linux_64_meta_yaml": {
  "about": {
   "description": "The Soapy UHD project provides a plugin module to use UHD devices within the\nSoapySDR API and software that supports SoapySDR. In addition, the project\nprovides a UHD module to use any SoapySDR device within the UHD API and UHD\nsupported software.\n",
   "dev_url": "https://github.com/pothosware/SoapyUHD",
   "doc_url": "https://github.com/pothosware/SoapyUHD/wiki",
   "home": "https://github.com/pothosware/SoapyUHD/wiki",
   "license": "GPL-3.0-or-later",
   "license_family": "GPL",
   "license_file": "COPYING",
   "summary": "Soapy SDR plugin for UHD supported SDR devices"
  },
  "build": {
   "number": "3",
   "run_exports": [
    "soapysdr-module-uhd"
   ]
  },
  "extra": {
   "recipe-maintainers": [
    "ryanvolz"
   ]
  },
  "package": {
   "name": "soapysdr-module-uhd",
   "version": "0.4.1"
  },
  "requirements": {
   "build": [
    "cxx_compiler_stub",
    "cmake",
    "make"
   ],
   "host": [
    "boost-cpp",
    "soapysdr",
    "uhd"
   ],
   "run": [
    "boost-cpp"
   ]
  },
  "source": {
   "patches": [
    "0001-SoapyUHDDevice-implemented-getNativeDeviceHandle.patch",
    "0002-UHDSoapyDevice-Add-cctype-header-needed-for-std-isdi.patch",
    "0003-Fix-for-getBandwidthRange-for-SOAPY_SDR_RX.patch",
    "0004-Fix-get_tree-for-X300-on-UHD-4.0.patch"
   ],
   "sha256": "9779cce2e732cd41905b6cf8ea85edbbf51b1ac918e6180bd4891eebb4c8d085",
   "url": "https://github.com/pothosware/SoapyUHD/archive/soapy-uhd-0.4.1.tar.gz"
  },
  "test": {
   "commands": [
    "test -f $PREFIX/lib/SoapySDR/modules0.8/libuhdSupport.so",
    "test -f $PREFIX/lib/uhd/modules/libsoapySupport.so",
    "SoapySDRUtil --info"
   ]
  }
 },
