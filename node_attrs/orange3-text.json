 "linux_64_meta_yaml": {
  "about": {
   "description": "Orange add-on for text mining. It provides access to publicly available\ndata, like NY Times, Twitter and PubMed. Further, it provides tools for\npreprocessing, constructing vector spaces (like bag-of-words and topic\nmodeling) and visualizations like word cloud and geo map. All features\ncan be combined with powerful data mining techniques from the Orange\ndata mining framework.\n",
   "dev_url": "https://github.com/biolab/orange3-text",
   "doc_url": "http://orange3-text.readthedocs.io",
   "home": "https://github.com/biolab/orange3-text",
   "license": "AGPL-3.0-only",
   "license_family": "AGPL",
   "license_file": "LICENSE",
   "summary": "Orange3 add-on for analyzing textual data."
  },
  "build": {
   "noarch": "python",
   "number": "0",
   "script": [
    "rm -f orangecontrib/text/widgets/tests/data/good/sample_txt_\u017E.txt",
    "rm -f orangecontrib/text/widgets/tests/data/documents/good/sample_txt_\u017E.txt",
    "/usr/share/miniconda3/envs/run_env/conda-bld/_h_env_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_pla/bin/python -m pip install . -vv"
   ]
  },
  "extra": {
   "recipe-maintainers": [
    "nikicc",
    "astaric",
    "ajdapretnar",
    "primozgodec"
   ]
  },
  "package": {
   "name": "orange3-text",
   "version": "1.6.2"
  },
  "requirements": {
   "host": [
    "python >=3.6",
    "pip"
