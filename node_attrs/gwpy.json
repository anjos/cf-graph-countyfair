 "linux_64_meta_yaml": {
  "about": {
   "description": "GWpy is a collaboration-driven Python package providing tools for\nstudying data from ground-based gravitational-wave detectors.\n\nGWpy provides a user-friendly, intuitive interface to the common\ntime-domain and frequency-domain data produced by the LIGO and Virgo\nobservatories and their analyses, with easy-to-follow tutorials at each\nstep.\n",
   "dev_url": "https://github.com/gwpy/gwpy/",
   "doc_url": "https://gwpy.github.io/docs/stable/",
   "home": "https://gwpy.github.io",
   "license": "GPL-3.0-or-later",
   "license_family": "GPL",
   "license_file": "LICENSE",
   "summary": "A python package for gravitational-wave astrophysics"
  },
  "build": {
   "entry_points": [
    "gwpy-plot = gwpy.cli.gwpy_plot:main"
   ],
   "noarch": "python",
   "number": "0",
   "script": "-m pip install . -vv"
  },
  "extra": {
   "recipe-maintainers": [
    "duncanmmacleod"
   ]
  },
  "package": {
   "name": "gwpy",
   "version": "2.0.1"
  },
  "requirements": {
   "host": [
    "pip",
    "python",
    "setuptools >=20.2.2"
   ],
   "run": [
    "astropy >=3.0.0",
    "dqsegdb2",
    "gwdatafind",
    "gwosc >=0.5.3",
    "h5py >=1.3",
    "ligo-segments >=1.0.0",
    "ligotimegps >=1.2.1",
    "matplotlib-base >=3.1.0",
    "numpy >=1.12.0",
    "python",
    "python-dateutil",
    "scipy >=1.2.0",
    "tqdm >=4.10.0"
   ]
  },
  "source": {
   "sha256": "3f40c36218c9766b6ad96d8831f8e019344995ef92793445a476fdd9ff26bb6c",
