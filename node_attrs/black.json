    "xhochy"
   ]
  },
  "outputs": [
   {
    "build": {
     "entry_points": [
      "black = black:patched_main"
     ],
     "noarch": "python"
    },
    "name": "black",
    "requirements": {
     "host": [
      "python >=3.6",
      "pip",
      "setuptools_scm"
     ],
     "run": [
      "python >=3.6",
      "platformdirs >=2",
      "click >=8.0.0",
      "dataclasses >=0.6",
      "mypy_extensions >=0.4.3",
      "pathspec >=0.9,<1",
      "tomli >=1.1.0",
      "typed-ast >=1.4.2",
      "typing_extensions >=3.10,!=3.10.0.1"
     ]
    },
    "script": "build_base.sh",
    "test": {
     "commands": [
      "black --help",
      "pip check"
     ],
     "requires": [
      "pip"
     ]
    }
   },
   {
    "build": {
     "entry_points": [
      "blackd = blackd:patched_main"
     ],
     "noarch": "python"
    },
    "name": "blackd",
    "requirements": {
     "run": [
      "black",
      "aiohttp >=3.7.4",
      "aiohttp-cors >=0.4.0"
     ]
    },
    "test": {
     "commands": [
      "black --help",
      "blackd --help"
     ]
    }
   },
   {
    "build": {
     "noarch": "python"
    },
